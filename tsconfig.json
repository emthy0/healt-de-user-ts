{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "*"
      ],
      "@store/*": [
        "store/*"
      ],
      "@types/*": [
        "types/*"
      ],
      "@services/*": [
        "services/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@redux/*": [
        "redux/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@providers/*": [
        "providers/*"
      ],
      "@contracts/*": [
        "contracts/*"
      ],
    },
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
